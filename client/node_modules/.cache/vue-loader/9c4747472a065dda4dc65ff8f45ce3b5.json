uerySelector(\".loading\").style.display = \"block\";\r\n      \r\n\r\n      setTimeout(function () {\r\n        window.location.reload();\r\n        document.querySelector(\".loading\").style.display = \"none\";\r\n      }, 1500);\r\n    },\r\n    \r\n  },\r\n  components: { ordercomponent },\r\n  name: \"About\",\r\n};\r\n</script>\r\n"]}]}